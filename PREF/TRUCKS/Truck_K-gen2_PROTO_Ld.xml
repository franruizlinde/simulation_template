      <category name="forksCamera">
        <pref name="xRotation">-180</pref>
        <pref name="yRotation">1</pref>
        <pref name="zRotation">-0.7</pref>
        <pref name="xTranslation">-0.066</pref>
        <pref name="yTranslation">-0.059</pref>
        <pref name="zTranslation">-0.255</pref>
        <!-- <pref name="offsetDetection_front">0.03</pref> -->
      </category>
      <category name="mastCamera">
        <pref name="xRotation">0</pref>
        <pref name="yRotation">0</pref>
        <pref name="zRotation">0</pref>
        <pref name="xTranslation">-0.22</pref>
        <pref name="yTranslation">0</pref>
        <pref name="zTranslation">2.58</pref>
      </category>
    </category>
  </category>
  <category name="perception">
    <pref name="forksType">mySTLForksFile</pref>
  </category>
  <category name="safety">
    <pref name="forksLowInductiveHeight">0.02</pref>
    <pref name="cabinLowInductiveHeight">0.45</pref>
  </category>
</Preferences>
