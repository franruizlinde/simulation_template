    <pref name="cap">Auto</pref>
    <pref name="height">Auto</pref>
  </category>
  <category name="sensors">
    <category name="encoderVerticalForks">
      <pref name="filteringRatio">1</pref>
      <pref name="physicalMeasureLowStop">0.062</pref>
      <pref name="physicalMeasureHighStop">6.5</pref>
      <pref name="sensorValueLowStop">60</pref>
      <pref name="sensorValueHighStop">6500</pref>
      <category name="auxiliarySensor">
        <pref name="filteringRatio">1</pref>
        <pref name="physicalMeasureLowStop">0</pref>
        <pref name="physicalMeasureHighStop">1.785</pref>
        <pref name="sensorValueLowStop">2</pref>
        <pref name="sensorValueHighStop">1791</pref>
      </category>
    </category>
    <category name="encoderAuxiliaryMastForks">
      <pref name="filteringRatio">1</pref>
      <pref name="physicalMeasureLowStop">0</pref>
      <pref name="physicalMeasureHighStop">1.785</pref>
      <pref name="sensorValueLowStop">2</pref>
      <pref name="sensorValueHighStop">1791</pref>
    </category>
    <category name="encoderCabin">
      <pref name="filteringRatio">1</pref>
      <pref name="physicalMeasureLowStop">0.062</pref>
      <pref name="physicalMeasureHighStop">4.710</pref>
      <pref name="sensorValueLowStop">60</pref>
      <pref name="sensorValueHighStop">4710</pref>
    </category>
    <category name="encoderRetractForks">
      <pref name="filteringRatio">1</pref>
      <pref name="retractLeftRange">1.519</pref>
