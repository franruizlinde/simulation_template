<?xml version="1.0" encoding="utf-8"?>
<Preferences sdk="4.11.1">
  <category name="general">
    <pref name="modelFileName">LINDE_K-gen2</pref>
  </category>
  <category name="odometrie">
    <pref name="alpha0">-1</pref>
  </category>
  <category name="traction_speed_control">
    <pref name="zero_value">0</pref>
  </category>
  <category name="turret_position_control">
    <pref name="zero_value">0</pref>
  </category>
  <category name="battery">
    <pref name="hardware">CAN</pref>
  </category>
  <category name="navigationLaser">
    <pref name="inhibitedRayIndex">Auto</pref>
    <pref name="inhibitedRayValue">Auto</pref>
    <pref name="numberOfRayDuringCalibration">Auto</pref>
  </category>
  <category name="navigationLaser2">
    <pref name="laserRobCap">0.0</pref>
  </category>
  <category name="frontLaser">
    <pref name="inhibitedRayIndex">Auto</pref>
    <pref name="inhibitedRayValue">Auto</pref>
    <pref name="numberOfRayDuringCalibration">Auto</pref>
  </category>
  <category name="rearLaser">
    <pref name="inhibitedRayIndex">Auto</pref>
    <pref name="inhibitedRayValue">Auto</pref>
    <pref name="numberOfRayDuringCalibration">Auto</pref>
    <pref name="phi">Auto</pref>
  </category>
  <category name="rearLeftLaser">
    <pref name="inhibitedRayIndex">Auto</pref>
    <pref name="inhibitedRayValue">Auto</pref>
    <pref name="numberOfRayDuringCalibration">Auto</pref>
    <pref name="phi">Auto</pref>
  </category>
  <category name="rearRightLaser">
    <pref name="inhibitedRayIndex">Auto</pref>
